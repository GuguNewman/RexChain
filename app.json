{
  "name": "Prescrypt Chain",
  "description": "Made Cryptography to Mexican Health Environ",
  "scripts": {
    "postdeploy": "bin/review_app_init"
  },
  "env": {
    "HEROKU_APP_NAME": {
      "required": true
    },
    "HEROKU_PARENT_APP_NAME": {
      "required": true
    },
    "AUTO_MIGRATE": {
      "required": true
    },
    "SECRET_KEY": {
      "required": true
    },
    "DEBUG_STATE": {
      "required": true
    },
    "PRODUCTION": {
      "required": true
    },
    "BLOCK_SIZE": {
      "required": true
    },
    "DISABLE_COLLECTSTATIC": {
      "required": true
    },
    "SECURE_SSL_REDIRECT": {
      "required": true
    },
    "BLOCKCYPHER_API_TOKEN": {
      "required": true
    },
    "CHAIN": {
      "required": true
    },
    "BASE_POE_URL": {
      "required": true
    },
    "ALLOWED_HOSTS": {
      "required": true
    },
    "LOAD_FIXTURES": "True"
  },
  "formation": {
    "web": {
      "quantity": 1
    },
    "worker": {
      "quantity": 1,
      "size": "free"
    }
  },
  "addons": [
    "papertrail",
    "heroku-postgresql",
    "heroku-redis"
  ],
  "buildpacks": [
    {
      "url": "heroku/python"
    }
  ]
}
